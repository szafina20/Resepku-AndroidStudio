<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Warna latar belakang tombol -->
    <solid android:color="#FF9933" />

    <!-- Mengatur sudut melengkung -->
    <corners android:radius="16dp" />

    <!-- Opsional: Padding di dalam tombol -->
    <padding
        android:left="12dp"
        android:right="12dp"
        android:top="8dp"
        android:bottom="8dp" />

    <!-- Opsional: Border atau garis tepi -->
    <stroke
        android:width="2dp"
        android:color="#FF6600" />
</shape>
